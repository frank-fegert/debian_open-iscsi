#!/bin/sh
# postrm script for open-iscsi

set -e

case "$1" in
    purge)
        if [ -d /etc/iscsi ] ; then
            rm -rf /etc/iscsi
        fi
    ;;

    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    ;;

    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0
